{
    "name": "jzakki/symfony-api-mapper",
    "description": "This is a package for mapping responded objects from third-party APIs to symfony corresponding entity objects.",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "SymfonyApiMapper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SymfonyApiMapper\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "javadzakki"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "myclabs/php-enum": "^1.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/error-handler": "6.2.x-dev",
        "symfony/yaml": "6.2.x-dev",
        "symfony/config": "6.2.x-dev",
        "symfony/http-kernel": "6.2.x-dev",
        "symfony/dependency-injection": "6.2.x-dev"
    }
}
